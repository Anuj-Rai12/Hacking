<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/refer_navigation"
    app:startDestination="@id/referFragment">

    <fragment
        android:id="@+id/referFragment"
        android:name="com.uptodd.uptoddapp.ui.refer.refer.ReferFragment"
        android:label="fragment_refer"
        tools:layout="@layout/fragment_refer" >
        <action
            android:id="@+id/action_referFragment_to_referListFragment"
            app:destination="@id/referListFragment" />
    </fragment>
    <fragment
        android:id="@+id/referListFragment"
        android:name="com.uptodd.uptoddapp.ui.refer.referlist.ReferListFragment"
        android:label="fragment_refer_list"
        tools:layout="@layout/fragment_refer_list" >
        <action
            android:id="@+id/action_referListFragment_to_referDetailsFragment"
            app:destination="@id/referDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/referDetailsFragment"
        android:name="com.uptodd.uptoddapp.ui.refer.referdetails.ReferDetailsFragment"
        android:label="ReferDetailsFragment" />
</navigation>